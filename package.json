{
  "name": "flatlatte-next",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://okvianey.github.io/flatlatte-next",
  "scripts": {
    "lint": "eslint",
    "dev": "next dev",
    "build": "next build",
    "preview": "cd out && python3 -m http.server 3000",
